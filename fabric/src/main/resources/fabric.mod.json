{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "https://github.com/fubira/HorseInfoReloaded/",
    "issues": "https://github.com/fubira/HorseInfoReloaded/issues"
  },
  "license": "${mod_license}",
  "environment": "client",
  "entrypoints": {
    "client": [
      "${mod_group_id}.fabric.HorseInfoMod"
    ]
  },
  "depends": {
    "fabricloader": ">=0.16.0",
    "fabric": "*",
    "cloth-config2": "*",
    "minecraft": "${minecraft_version_range}",
    "java": ">=21"
  }
}
